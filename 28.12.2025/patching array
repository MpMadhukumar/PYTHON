class Solution(object):
    def minPatches(self, nums, n):
        """
        :type nums: List[int]
        :type n: int
        :rtype: int
        """
        ans = 0
        k = 0
        for num in nums+[n]:
            while k < num - 1 and k < n:
                k += k + 1
                ans += 1
            k += num
        return ans
