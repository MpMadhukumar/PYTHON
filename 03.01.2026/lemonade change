#Python Code
class Solution(object):
    def lemonadeChange(self, bills):
        """
        :type bills: List[int]
        :rtype: bool
        """
        f , t = 0, 0
        for bill in bills:
            if bill == 5:
                f += 1
            elif bill == 10:
                if f >= 1:
                    f -= 1
                    t += 1
                else:
                    return False
            else:
                if f >= 1 and t >= 1:
                    t -= 1
                    f -= 1
                elif f >= 3:
                    f -= 3
                else:
                    return False
        return True
    
        
